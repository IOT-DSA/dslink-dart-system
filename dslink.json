{
  "name": "dslink-dart-system",
  "version": "1.1.0",
  "description": "System DSLink",
  "main": "bin/run.dart",
  "engines": {
    "dart": ">=1.15.0"
  },
  "getDependencies": [],
  "configs": {
    "broker": {
      "type": "url"
    },
    "name": {
      "type": "string",
      "default": "System"
    },
    "token": {
      "type": "string"
    },
    "linux_use_free_command": {
      "type": "bool",
      "default": true
    },
    "offset_memory_disk_cache": {
      "type": "bool",
      "default": true
    },
    "linux_use_slow_cpu_usage": {
      "type": "bool",
      "default": false
    },
    "log_statistic_exceptions": {
      "type": "bool",
      "default": false
    }
  }
}
